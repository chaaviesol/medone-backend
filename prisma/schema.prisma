// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}








  model user_details{
    id                      Int @id @default(autoincrement())
    name                    String?  
    phone_no                String?  
    email                   String?  
    password                String?
    datetime                DateTime
    pincode                 String?
    ageGroup                String?
    gender                  String?
    updatedDate             DateTime?
    last_active             DateTime?
    status                  String?
    image                   String? 
    health_condition        Json?
    height                  String?
    weight                  String?
    googleId                String?   
     token                   String?  
    notificationTime        String?               
    customer_cart           customer_cart[]
    sales_order             sales_order[]
    daily_routine           dailyRoutine[]
    medicine_timetable      medicine_timetable[]
  }
model generic_product {
  id                                                                      Int                     @id @default(autoincrement())
  name                                                                    String? 
  generic_name                                                            String?   
  composition                                                             Json?
  product_type                                                            String?              
  description                                                             String?                  
  category                                                                Json?                                   
  created_by                                                              String?                  
  created_date                                                            DateTime?                
  is_active                                                               String?                  
  updated_by                                                              String?                  
  updated_date                                                            DateTime?              
  images                                                                  Json?
  mrp                                                                     Int?  
  brand                                                                   String?
  hsn                                                                     String?                  
  customer_cart                                                           customer_cart[]
  sales_list                                                              sales_list[]
 
}
model customer_cart {
  id                     Int            @id @default(autoincrement())
  user_id                Int
  prod_id                Int
  quantity               Int
  userid                 user_details ?@relation(fields: [user_id],references: [id])
  generic_prodid         generic_product ?@relation(fields: [prod_id],references: [id])
  created_date           DateTime? 
}

model productcategory{
  id                     Int                      @id @default(autoincrement()) 
  category               String?                 
  image                  String?
  status                 Boolean?
  created_date           DateTime?
  created_by             Int?
  modified_date          DateTime?
  
}

model sales_order {
  sales_id               Int                      @id @default(autoincrement())
  so_number              String?                  @unique(map: "so_number") 
  total_amount           Decimal?                 @db.Decimal(10, 2)                              
  so_status              String? 
  order_type             String?               
  remarks                String? 
  delivery_address       String?
  district               String?
  city                   String?                
  delivery_date          DateTime?                              
  created_date           DateTime?              
  updated_by             Int?                 
  updated_date           DateTime?
  contact_no             String?              
  customer_id            Int?   
  pincode                Int?
  prescription_image     Json?   
  patient_name           String?
  doctor_name            String?         
  sales_list             sales_list[]
  sales_invoice          sales_invoice[]
  users                  user_details? @relation(fields: [customer_id],references: [id])
}

model sales_invoice{
  id                   Int          @id @default(autoincrement())
  sales_id             Int?
  created_date         DateTime?
  sold_by              String?
  sales_order          sales_order?  @relation(fields: [sales_id], references: [sales_id])
  medicine_timetable   medicine_timetable[]
}
model sales_list {
  id              Int               @id @default(autoincrement())
  sales_id        Int
  product_id      Int
  order_qty       Int?
  net_amount      Int?
  selling_price   Int?
  batch_no        String?
  pharmacy_name   String?         
  created_date    DateTime?                     
  sales_order     sales_order?  @relation(fields: [sales_id], references: [sales_id])
  generic_prodid  generic_product? @relation(fields: [product_id],references: [id])
}
model dailyRoutine{
  id                    Int           @id @default(autoincrement())
  userId                Int?
  routine               Json?
  created_date          DateTime?
  userid                user_details?  @relation(fields: [userId],references: [id])
}

model medicine_timetable {
  id                    Int               @id @default(autoincrement())
  userId                Int?
  medicine              Json?
  medicine_type         String?
  image                 String?
  startDate             String?
  no_of_days            String?
  afterFd_beforeFd      String?
  totalQuantity         String?
  timing                Json?
  timeInterval          String?
  takingQuantity        String?
  daysInterval          String? 
  created_date          DateTime?
  active_status         String?
  sales_invoiceid       Int?
  app_flag              Boolean?
  user_id               user_details? @relation(fields: [userId],references: [id])
   sales_invoice         sales_invoice? @relation(fields: [sales_invoiceid],references: [id])
  // user_id               user_details? @relation(fields: [userId],references: [id])
}

model medication_records{
  id                    Int                 @id @default(autoincrement())
  userId                Int?
  timetable_id          Int?
  status                String?    //snooze or stop notifications is recorded
  taken_time            String?
  taken_status          String?
  created_date          DateTime?
}

model medicines{
  id                     Int                      @id @default(autoincrement()) 
  name                   String?
  category               String?
  status                 String? 
  created_date           DateTime?
  created_by             Int?
  
}
model notification{
  id                     Int                       @id @default(autoincrement())
  user_id                Int?
  message                String?
  status                 String?
  view_status            String?
  created_date           DateTime @default(now())
}

model hospital_details{
  id                         Int  @id @default(autoincrement())
  name                       String?
  address                    Json?
  licence_no                 String?
  rating                     String?
  feature                    Json?
  datetime                   DateTime
  photo                      Json?
  speciality                 Json?
  focusarea                  Json?
  contact_no                 String?
  onlinebooking              String?
  unique_id                  String?
  type                       String?  
  pincode                    Int?  
  about                      String?
  email                      String? 
  password                   String? 
  updatedDate                DateTime?  
  last_active                DateTime?  
  status                     String?  
  featured_partner           Boolean?                                                        
  // doctor_hospital_id         doctor_hospital[]
  // interacteduser             hospital_interacteduser[]
  // feedback                   hospital_feedback[]
 }


 model user_feedback{
  id                           Int  @id @default(autoincrement())
  userId                       Int?
  medicineId                   Int?
  feedback                     Json? // title , comment , userName 
  createdDate                  DateTime
 }


 model remainder{
  id                          Int  @id @default(autoincrement())
  quotes                      String?
  created_date                DateTime
 }

 model pharmacy_details{
    id                       Int @id @default(autoincrement())
    name                     String?
    phone_no                 String?
    address                  String?
    lisence_no               String?
    datetime                 DateTime
    email                    String?
    password                 String?
    pincode                  Int?
    created_by               Int?    
  }