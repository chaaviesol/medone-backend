// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}








  model user_details{
    id                      Int @id @default(autoincrement())
    name                    String?  
    phone_no                String?  
    email                   String?  
    password                String?
    datetime                DateTime
    pincode                 String?
    ageGroup                String?
    gender                  String?
    updatedDate             DateTime?
    last_active             DateTime?
    status                  String?
    image                   String? 
    health_condition        Json?
    height                  String?
    weight                  String?
    googleId                String?   
     token                   String?  
    notificationTime        String?               
    // userId                  quotation[]
    // interactedLabuser       lab_interacteduser[]
    // interactedDoctoruser    doctor_interacteduser[]
    // interactedHospitaluser  hospital_interacteduser[]
    // labsearchdata           lab_searchdata[]
    // labfeedback             lab_feedback[]
    // doctorsearchdata        doctor_searchdata[]
    // doctorfeedback          doctor_feedback[]
    // hospitalfeedback        hospital_feedback[]
    // hospitalsearchdata      hospital_searchdata[]
    // customer_cart           customer_cart[]
    // sales_order             sales_order[]
    // second_opinion_data     second_opinion_data[]
    // query_data              query_data[]
    daily_routine           dailyRoutine[]
    medicine_timetable      medicine_timetable[]
  }
model generic_product {
  id                                                                      Int                     @id @default(autoincrement())
  name                                                                    String?                  
  description                                                             String?                  
  category                                                                Json?                                   
  created_by                                                              String?                  
  created_date                                                            DateTime?                
  is_active                                                               String?                  
  updated_by                                                              String?                  
  updated_date                                                            DateTime?              
  images                                                                  Json?
  mrp                                                                     Int?  
  brand                                                                   String?                  
  // customer_cart                                                           customer_cart[]
  // sales_list                                                              sales_list[]  
}
model dailyRoutine{
  id                    Int           @id @default(autoincrement())
  userId                Int?
  routine               Json?
  created_date          DateTime?
  userid                user_details?  @relation(fields: [userId],references: [id])
}

model medicine_timetable {
  id                    Int               @id @default(autoincrement())
  userId                Int?
  medicine              Json?
  medicine_type         String?
  image                 String?
  startDate             String?
  no_of_days            String?
  afterFd_beforeFd      String?
  totalQuantity         String?
  timing                Json?
  timeInterval          String?
  takingQuantity        String?
  daysInterval          String? 
  created_date          DateTime?
  active_status         String?
  app_flag              Boolean?
  user_id               user_details? @relation(fields: [userId],references: [id])
}

model medication_records{
  id                    Int                 @id @default(autoincrement())
  userId                Int?
  timetable_id          Int?
  status                String?    //snooze or stop notifications is recorded
  taken_time            String?
  taken_status          String?
  created_date          DateTime?
}

model medicines{
  id                     Int                      @id @default(autoincrement()) 
  name                   String?
  category               String?
  status                 String? 
  created_date           DateTime?
  created_by             Int?
  
}
model notification{
  id                     Int                       @id @default(autoincrement())
  user_id                Int?
  message                String?
  status                 String?
  view_status            String?
  created_date           DateTime @default(now())
}

model hospital_details{
  id                         Int  @id @default(autoincrement())
  name                       String?
  address                    Json?
  licence_no                 String?
  rating                     String?
  feature                    Json?
  datetime                   DateTime
  photo                      Json?
  speciality                 Json?
  focusarea                  Json?
  contact_no                 String?
  onlinebooking              String?
  unique_id                  String?
  type                       String?  
  pincode                    Int?  
  about                      String?
  email                      String? 
  password                   String? 
  updatedDate                DateTime?  
  last_active                DateTime?  
  status                     String?  
  featured_partner           Boolean?                                                        
  // doctor_hospital_id         doctor_hospital[]
  // interacteduser             hospital_interacteduser[]
  // feedback                   hospital_feedback[]
 }


 model user_feedback{
  id                           Int  @id @default(autoincrement())
  userId                       Int?
  medicineId                   Int?
  feedback                     Json? // title , comment , userName 
  createdDate                  DateTime
 }


 model remainder{
  id                          Int  @id @default(autoincrement())
  quotes                      String?
  created_date                DateTime
 }