// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user_details{
  id                      Int @id @default(autoincrement())
  name                    String?  
  phone_no                String?  
  email                   String?  
  password                String?
  datetime                DateTime
  pincode                 String?
  ageGroup                String?
  gender                  String?
  updatedDate             DateTime?
  last_active             DateTime?
  status                  String?
  image                   String? 
  health_condition        Json?
  height                  String?
  weight                  String?
  googleId                String?   
  token                   String?  
  notificationTime        String?               
  customer_cart           customer_cart[]
  sales_order             sales_order[]
  daily_routine           dailyRoutine[]
  medicine_timetable      medicine_timetable[]
  labtest_cart            labtest_cart[]
  labtest_order           labtest_order[]
  hospitalAssist_service    hospitalAssist_service[]
  homeCare_Service          homeCare_Service[]
  physiotherapist_service   physiotherapist_service[]    
}

model generic_product {
  id                                                                      Int                     @id @default(autoincrement())
  name                                                                    String? 
  generic_name                                                            String?   
  composition                                                             Json?
  product_type                                                            String?              
  description                                                             String?                  
  category                                                                Json?                                   
  created_by                                                              String?                  
  created_date                                                            DateTime?                
  is_active                                                               String?                  
  updated_by                                                              String?                  
  updated_date                                                            DateTime?              
  images                                                                  Json?
  mrp                                                                     Int?  
  brand                                                                   String?
  hsn                                                                     String?
  prescription_required                                                   Boolean?
  unit_of_measurement                                                     String?
  medicine_unit                                                           Int?
  selling_price                                                           Decimal?                 @db.Decimal(10, 2)                  
  customer_cart                                                           customer_cart[]
  sales_list                                                              sales_list[]
 
}
model customer_cart {
  id                     Int            @id @default(autoincrement())
  user_id                Int
  prod_id                Int
  quantity               Int
  userid                 user_details ?@relation(fields: [user_id],references: [id])
  generic_prodid         generic_product ?@relation(fields: [prod_id],references: [id])
  created_date           DateTime? 
}

model productcategory{
  id                     Int                      @id @default(autoincrement()) 
  category               String?                 
  image                  String?
  status                 Boolean?
  created_date           DateTime?
  created_by             Int?
  modified_date          DateTime?
  
}

model sales_list {
  id              Int               @id @default(autoincrement())
  sales_id        Int
  product_id      Int
  order_qty       Int?
  net_amount      Decimal?                 @db.Decimal(10, 2)
  selling_price   Int?
  discount        Int?
  batch_no        String?
  pharmacy_name   String?         
  created_date    DateTime?                     
  sales_order     sales_order?  @relation(fields: [sales_id], references: [sales_id])
  generic_prodid  generic_product? @relation(fields: [product_id],references: [id])
}

model dailyRoutine{
  id                    Int           @id @default(autoincrement())
  userId                Int?
  routine               Json?
  created_date          DateTime?
  userid                user_details?  @relation(fields: [userId],references: [id])
}

model medicine_timetable {
  id                    Int               @id @default(autoincrement())
  userId                Int?
  medicine              Json?
  medicine_type         String?
  image                 String?
  startDate             String?
  no_of_days            String?
  afterFd_beforeFd      String?
  totalQuantity         String?
  timing                Json?
  timeInterval          String?
  takingQuantity        String?
  daysInterval          String? 
  created_date          DateTime?
  active_status         String?
  sales_id              Int?
  app_flag              Boolean?
  user_id               user_details? @relation(fields: [userId],references: [id])
  sales_order           sales_order?  @relation(fields: [sales_id], references: [sales_id])
}

model medication_records{
  id                    Int                 @id @default(autoincrement())
  userId                Int?
  timetable_id          Int?
  status                String?    //snooze or stop notifications is recorded
  taken_time            String?
  taken_status          String?
  created_date          DateTime?
}

model medicines{
  id                     Int                      @id @default(autoincrement()) 
  name                   String?
  category               String?
  status                 String? 
  created_date           DateTime?
  created_by             Int?
  
}
model notification{
  id                     Int                       @id @default(autoincrement())
  user_id                Int?
  message                String?
  status                 String?
  view_status            String?
  notificationTime       String?     
  created_date           DateTime @default(now())
}

model hospital_details{
  id                         Int  @id @default(autoincrement())
  name                       String?
  address                    Json?
  licence_no                 String?
  rating                     String?
  feature                    Json?
  datetime                   DateTime
  photo                      Json?
  speciality                 Json?
  focusarea                  Json?
  contact_no                 String?
  onlinebooking              String?
  unique_id                  String?
  type                       String?  
  pincode                    Int?  
  about                      String?
  email                      String? 
  password                   String? 
  updatedDate                DateTime?  
  last_active                DateTime?  
  status                     String?  
  featured_partner           Boolean?                                                        
  
}


model user_feedback{
  id                           Int  @id @default(autoincrement())
  userId                       Int?
  medicineId                   Int?
  feedback                     Json? // title , comment , userName 
  createdDate                  DateTime
 }


model remainder{
  id                          Int  @id @default(autoincrement())
  quotes                      String?
  created_date                DateTime
 }

model pharmacy_details{
    id                       Int @id @default(autoincrement())
    name                     String?
    phone_no                 String?
    address                  Json?
    lisence_no               String?
    datetime                 DateTime
    email                    String?
    password                 String?
    pincode                  Int?
    created_by               Int? 
    token                    String?
    temp_otp                 Int?
    pharmacy_assign          pharmacy_assign[]
    pharmacy_medicines       pharmacy_medicines[]
    sales_order              sales_order[]   
  }

model pharmacy_assign{
  id                       Int @id @default(autoincrement())
  pharmacy_id              Int
  sales_id                 Int
  status                   String?
  created_date             DateTime?
  Stmodified_date          DateTime?
  pharm_id                 pharmacy_details? @relation(fields: [pharmacy_id],references: [id])    
  salesorderid             sales_order? @relation(fields: [sales_id],references: [sales_id])


  }

model sales_order {
  sales_id               Int                      @id @default(autoincrement())
  so_number              String?                  @unique(map: "so_number") 
  total_amount           Decimal?                 @db.Decimal(10, 2)                              
  so_status              String? 
  order_type             String?               
  remarks                String? 
  delivery_address       String?
  delivery_location      Json?
  district               String?
  city                   String?                
  delivery_date          DateTime?                              
  created_date           DateTime?              
  updated_by             Int?                 
  updated_date           DateTime?
  contact_no             String?              
  customer_id            Int?   
  pincode                Int?
  prescription_image     Json?   
  patient_name           String?
  doctor_name            String? 
  pharmacy_id            Int?
  otp                    Int?        
  sales_list             sales_list[]
  users                  user_details? @relation(fields: [customer_id],references: [id])
  pharmacy               pharmacy_details? @relation(fields: [pharmacy_id],references: [id])
  pharmacy_assign        pharmacy_assign[]
  delivery_assign        delivery_assign[]
  medicine_timetable     medicine_timetable[]  
}

model pharmacy_medicines{
  id                       Int @id @default(autoincrement())
  pharmacy_id              Int?
  product_ids              Json?
  created_date             DateTime?
  updated_date             DateTime?
  pharmacy                 pharmacy_details? @relation(fields: [pharmacy_id],references: [id])  
}

model pharmacy_notification{
  id                       Int @id @default(autoincrement())
  pharmacyId               Int?
  message                  String?
  view_status              String?
  created_date             DateTime
}

model delivery_partner{
  id                       Int @id @default(autoincrement())
  email                    String?
  password                 String?
  name                     String?
  vehicle_id               String?
  is_active                Boolean?
  pharmacy_ids             Json?
  created_date             DateTime?
  otp                      String?
  wallet                   Int?
  phone                    String?
  delivery_assign          delivery_assign[]
}



model delivery_assign{
  id                       Int @id @default(autoincrement())
  sales_id                 Int?
  deliverypartner_id       Int?
  status                   String?
  assigned_date            DateTime?
  picked_update            DateTime?  
  delivered_date           DateTime?
  prescription_analysis    String?
  stamp_statusUpdate       String?
  payment_method           String?
  credited_payment         String?
  delivery_partner         delivery_partner? @relation(fields: [deliverypartner_id],references: [id])
  sales_order              sales_order? @relation(fields: [sales_id],references: [sales_id])   
}


model lab_details{
  id                       Int @id @default(autoincrement())
  name                     String?
  phone_no                 String?
  email                    String?
  address                  String?
  pincode                  String?
  about                    String?
  created_date             DateTime?
  is_active                Boolean?
  test_ids                 Json?                  
  package_id               Json?
  labtest_order            labtest_order[]
}

model labtest_details{
  id                       Int @id @default(autoincrement())
  test_number              String?   @unique(map: "test_number")
  name                     String?
  mrp                      Int?
  description              String?
  is_active                Boolean?
  photo                    String?
  type                     String?
  category                 String? 
  home_collection          Boolean?
  gender                   String?
  age_group                String?
  created_date             DateTime?
}

model lab_packages {
  id                       Int @id @default(autoincrement())
  package_name             String?
  test_number              String?   @unique(map: "test_number2") 
  labtest_ids              Json?  
  about                    String?
  price                    Int?
  home_collection          Boolean?
  health_concern           String?
  created_date             DateTime?
  is_active                Boolean?
}


model labtest_cart {
  id                     Int            @id @default(autoincrement())
  user_id                Int
  test_number            String?        
  userid                 user_details ?@relation(fields: [user_id],references: [id])
  created_date           DateTime? 
}

model labtest_order {
  order_id               Int                      @id @default(autoincrement())
  order_number           String?                  @unique(map: "order_number") 
  total_amount           Decimal?                 @db.Decimal(10, 2)                              
  status                 String? 
  order_type             String?               
  remarks                String? 
  delivery_details       Json?
  delivery_location      Json?                                             
  created_date           DateTime?                               
  updated_date           DateTime?
  contact_no             String?
  prescription_image     Json?              
  customer_id            Int?   
  pincode                Int?
  patient_details        Json?
  doctor_name            String?
  test_collection        String?
  lab_id                 Int?          
  labtest_list           labtest_list[]
  users                  user_details? @relation(fields: [customer_id],references: [id])
  lab_details            lab_details? @relation(fields: [lab_id],references: [id])
}

model labtest_list {
  id              Int               @id @default(autoincrement())
  order_id        Int
  test_number     String?     
  created_date    DateTime?                      
  labtest_order   labtest_order?  @relation(fields: [order_id], references: [order_id])  
}

model doctor_details{
  id                         Int    @id @default(autoincrement())
  name                       String?
  second_name                String?
  phone_no                   String?
  phone_office               String?
  email                      String?
  password                   String?
  image                      Json?
  education_qualification    String?
  additional_qualification   String?
  specialization             String?
  additional_speciality      String?
  type                       String?
  gender                     String?
  address                    Json?
  experience                 Int?
  about                      String?
  rating                     String?
  datetime                   DateTime
  updatedDate                DateTime?
  registration_no            String?
  featured_partner           Boolean?
  sector                     String?
  pincode                    Int?
  status                     String?
  query                      Boolean?
  expert_opinion             Boolean?
  video_consultaion          Boolean?
  last_active                DateTime?  
}

model assist_details{
  id                        Int               @id @default(autoincrement())
  type                      String?
  name                      String?
  gender                    String?
  dob                       String?
  address                   Json?
  qualification             String?
  shift                     String?
  photo                     String?
  phone_no                  String?
  location                  String?
  password                  String?
  created_date              DateTime?
  hospitalAssist_service    hospitalAssist_service[]
  homeCare_Service          homeCare_Service[]
  physiotherapist_service   physiotherapist_service[]
}

model homeCare_Service{
  id                   Int               @id @default(autoincrement())
  patient_name         String?
  patient_contact_no   String?
  patient_mobility     String?
  patient_gender       String?
  patient_age          String?
  start_date           DateTime?
  days_week            String?
  general_specialized  String?
  patient_location     Json?
  requirements         String?
  medical_documents    Json?
  price                Int?
  assist_id            Int?
  created_date         DateTime?
  assigned_date        DateTime?
  status               String?
  customer_id          Int?
  users                user_details? @relation(fields: [customer_id],references: [id])
  assist_details       assist_details?  @relation(fields: [assist_id], references: [id])  
}

model physiotherapist_service{
  id                   Int               @id @default(autoincrement())
  patient_name         String?
  patient_contact_no   String?
  patient_mobility     String?
  patient_gender       String?
  patient_age          String?
  start_date           DateTime?
  days_week            String?
  general_specialized  String?
  patient_location     Json?
  requirements         String?
  medical_documents    Json?
  price                Int?
  assist_id            Int?
  created_date         DateTime?
  assigned_date        DateTime?
  status               String?
  customer_id          Int?
  users                user_details? @relation(fields: [customer_id],references: [id])
  assist_details       assist_details?  @relation(fields: [assist_id], references: [id])
}

model hospitalAssist_service{
  id                   Int               @id @default(autoincrement())
  patient_name         String?
  patient_contact_no   String?
  patient_mobility     String?
  patient_gender       String?
  patient_age          String?
  assist_type          String?
  pickup_type          String?
  hospital_name        String?
  hospital_location    Json?
  start_date           DateTime?
  time                 String?
  days_week            String?
  patient_location    Json?
  requirements         String?
  medical_documents    Json?
  price                Int?
  assist_id            Int?
  created_date         DateTime?
  assigned_date        DateTime?
  vehicle_type         String?
  vehicle_id           String?
  status               String?
  customer_id          Int?
  users                user_details? @relation(fields: [customer_id],references: [id])
  assist_details       assist_details?  @relation(fields: [assist_id], references: [id])
}